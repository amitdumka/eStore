@model IEnumerable<eStore.Shared.Models.Accounts.LedgerEntry>

@{
    ViewData["Title"] = "LedgerEntries";
}

<h1>LedgerEntries</h1>

@*<p>
    <a asp-action="Create">Create New</a>
</p>*@
<table class="table">
    <thead>
        <tr>
