            <th>
                @Html.DisplayNameFor(model => model.VoucherType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Particulars)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AmountIn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AmountOut)
            </th>
