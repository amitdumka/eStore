            </th>
            <th>
                @Html.DisplayNameFor(model => model.EntryDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EntryType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReferanceId)
