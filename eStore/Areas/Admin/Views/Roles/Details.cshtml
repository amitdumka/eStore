@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>
@{ ViewData["Title"] = "Details"; }

<h1>List of User for Role: @ViewBag.RoleName </h1>

<table class="table table-striped table-bordered table-sm">
    <thead class="table-danger">
        <tr>
            @*<td>Id</td>*@
            <td>UserName</td>
            <td>Email</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var usr in Model)
        {
<tr>
    @*<td>@role.Id</td>*@
    <td>@usr.UserName</td>
    <td>
        @usr.Email
    </td>
</tr>}
    </tbody>
</table>
<div>
    <a asp-action="Index" class="btn btn-outline-default btn-sm">Back to List</a>
</div>